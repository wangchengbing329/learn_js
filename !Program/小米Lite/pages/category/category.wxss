/* pages/category/category.wxss */
/* 左边选择框 */
.container{
    height: 100%;
    display: flex;
    flex-direction: row;
}
::-webkit-scrollbar{  
    height: 0;
    width: 0;
    color: transparent;
  }
.cate-left{
    height: 100%;
    width: 20%;
    border-right: 1px solid #e7e8e9;
    -webkit-overflow-scrolling: none;
}
.cate{
    width: 100%;
    height: 100rpx;
    /* background-color: green; */
    margin: 20rpx 0;
    display: flex;
    justify-content: center;

}
.cate-name{
font-size: 30rpx;
text-align: center;
width: 100%;
height: 80rpx;
}
.cate-name.on{
    color:#fe6900
}